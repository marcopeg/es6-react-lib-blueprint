{
  "name": "es6-server-lib-blueprint",
  "version": "0.0.0",
  "description": "Blueprint for building an ES6 server library and publish it to NPM",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha --compilers js:babel-core/register",
    "test:watch": "npm test -- --watch",
    "build": "./node_modules/babel-cli/bin/babel.js src --out-dir lib",
    "build:watch": "./node_modules/babel-cli/bin/babel.js --watch src --out-dir lib",
    "clean": "./node_modules/rimraf/bin.js lib coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcopeg/es6-server-lib-blueprint.git"
  },
  "keywords": [
    "es6",
    "js2015",
    "js15",
    "npm",
    "module",
    "lib",
    "bundle",
    "blueprint",
    "transpile",
    "boilerplate",
    "library"
  ],
  "author": "Marco Pegoraro <marco.pegoraro@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcopeg/es6-server-lib-blueprint/issues"
  },
  "homepage": "https://github.com/marcopeg/es6-server-lib-blueprint#readme",
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.4.1",
    "mocha": "^2.3.4",
    "rimraf": "^2.5.0",
    "sinon": "^1.17.2"
  }
}
